{
  "name": "mojidraw-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack --node-env=production",
    "format": "prettier --write **/*.html **/*.js **/*.json **/*.md **/*.ts --ignore-path .lintignore",
    "lint": "eslint **/*.js **/*.json **/*.ts --ignore-path .lintignore",
    "start": "webpack serve --node-env=development"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.3.0",
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "css-loader": "^5.2.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.1",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
